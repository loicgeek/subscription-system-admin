{
    "name": "ntech-services/subscription-system-admin",
    "description": "A comprehensive Laravel package that provides a complete subscription system administration interface for Filament admin panels.",
    "keywords": [
        "laravel",
        "filament", 
        "subscription",
        "admin",
        "saas",
        "billing",
        "plans",
        "features",
        "coupons",
        "ntech-services"
    ],
    "homepage": "https://github.com/loicgeek/subscription-system-admin",
    "support": {
        "issues": "https://github.com/loicgeek/subscription-system-admin/issues",
        "source": "https://github.com/loicgeek/subscription-system-admin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Loic NGOU",
            "email": "loic.ngou98@gmail.com",
            "homepage": "https://github.com/loicgeek",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "ntech-services/subscription-system": "^0.0.14"
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "NtechServices\\SubscriptionSystemAdmin\\": "src/",
            "NtechServices\\SubscriptionSystemAdmin\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NtechServices\\SubscriptionSystemAdmin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NtechServices\\SubscriptionSystemAdmin\\SubscriptionSystemAdminServiceProvider"
            ],
            "aliases": {
                "SubscriptionSystemAdmin": "NtechServices\\SubscriptionSystemAdmin\\Facades\\SubscriptionSystemAdmin"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}