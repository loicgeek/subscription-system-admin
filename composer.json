{
    "version": "0.0.1",
    "name": "ntech-services/subscription-system-admin",
    "description": "This is my package subscription-system-admin",
    "keywords": [
        "loicgeek",
        "laravel",
        "subscription-system-admin"
    ],
    "homepage": "https://github.com/loicgeek/subscription-system-admin",
    "support": {
        "issues": "https://github.com/loicgeek/subscription-system-admin/issues",
        "source": "https://github.com/loicgeek/subscription-system-admin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "loic",
            "email": "loic.ngou98@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "ntech-services/subscription-system": "0.0.14"
    },
    "require-dev": {
        "larastan/larastan": "*",
        "laravel/pint": "*",
        "orchestra/testbench": "*",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-arch": "*",
        "pestphp/pest-plugin-laravel": "*",
        "phpstan/extension-installer": "*",
        "phpstan/phpstan-deprecation-rules": "*",
        "phpstan/phpstan-phpunit": "*",
        "spatie/laravel-ray": "*"
    },
    "autoload": {
        "psr-4": {
            "NtechServices\\SubscriptionSystemAdmin\\": "src/",
            "NtechServices\\SubscriptionSystemAdmin\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NtechServices\\SubscriptionSystemAdmin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NtechServices\\SubscriptionSystemAdmin\\SubscriptionSystemAdminServiceProvider",
            "NtechServices\\SubscriptionSystem\\SubscriptionServiceProvider"
            ],
            "aliases": {
                "SubscriptionSystemAdmin": "NtechServices\\SubscriptionSystemAdmin\\Facades\\SubscriptionSystemAdmin"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
